<aura:component controller='GetAccountdatausingRestApi' implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global" >
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />     
    <aura:attribute name="rec" type="String" /> 
    <ul> 
        <aura:iteration items="{!v.rec}" var="curr"> 
            <li> 
                <b style='background-color:white;color:red'>{!curr}</b>
            </li> 
        </aura:iteration> 
    </ul> 
    
</aura:component>